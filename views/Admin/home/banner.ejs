<div class="content mt-3">
  <div class="animated fadeIn">
      <div class="row">
          <div class="col-md-12">
              <div class="card">
                  <div class="card-header d-flex justify-content-between">
                      <div>
                        <strong class="card-title">Data Table</strong>
                      </div>
                      <div>
                        <!-- Button trigger modal -->
                        <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">Add property</button>
                      </div>
                  </div>
                  <div class="card-body">
                      <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                          <thead>
                          
                              <tr>
                                  <th>S.no</th>
                                  <th>PropertyName</th>
                                  <th>F_Image</th>
                                  <th>P_images</th>
                                  <th>Price</th>
                                  <th>Action</th>
                              </tr>
                       
                          </thead>
                          <tbody>
                               <%= i=1%>  
                               <% bannerdata.forEach((items)=>{   %> 
                              
                                    <tr>
                                  <td><%= i++ %></td>
                                  <td><%= items.property_name%></td>
                                  <td><img src="<%= items.front_image.url%>" alt=""></td>
                                  <td>
                                  <% for(let element of items.images){ %>
                                     <img src="<%= element.url %>" alt=""style="max-width: 30%;">
                                  <% } %>
                                  </td>
                                  <td><%= items.price%></td>
                                  <td>
                                    <a href="/admin/dashboard/bannerview/<%= items._id%>" class="btn btn-primary " style="margin: 3px;">view</a>
                                    <a href="/admin/dashboard/banneredit/<%= items._id%>"class="btn btn-primary" style="margin: 3px;">edit</a>
                                    <a href=""class="btn btn-primary" style="margin: 3px;">delete</a>
                                  </td>
                                    
                                  </td>
                                  
                              </tr>
                            <%   })  %>  
                          </tbody>
                      </table>
                  </div>
              </div>
          </div>
      </div>
  </div><!-- .animated -->


</div><!-- .content -->
</div><!-- /#right-panel -->



<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h1 class="modal-title fs-5" id="exampleModalLabel">Add Property Details</h1>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
            <form action="/multiple-upload" method="post" enctype="multipart/form-data">
              <div class="mb-3">
                <label for="">Property Front images</label>
                <input type="file" name="front_image" class="form-control">
              </div>
              <div class="mb-3">
                <label for=""></label> images</label>
                <input type="file" id="files" name="images" multiple class="form-control">
              </div>
              <div class="mb-3">
                <label for="">Status</label>
                <input type="text" name="status" class="form-control">
              </div>
            <div class="mb-3">
              <label for="">Property Name</label>
              <input type="text" name="property_name" class="form-control">
            </div>
            <div class="mb-3">
              <label for="">State</label>
              <select name="state" id="" class="form-select">
                  <option value="null">Select state</option>
                  <option value="Madhya Pradesh">Madhya Pradesh</option>
                  <option value="New delhi">New delhi</option>
                  <option value="Mumbai">Mumbai</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="">City</label>
              <select name="city" id="" class="form-select">
                  <option value="null">Select city</option>
                  <option value="Gwalior">Gwalior</option>
                  <option value="Bhopal">Bhopal</option>
                  <option value="Indore">Indore</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="">Address</label>
              <input type="text" name="address" class="form-control">
            </div>
            <div class="mb-3">
              <label for="">Description</label>
              <textarea name="descripation" class="form-control" id="" cols="30" rows="7"></textarea>
            </div>
            <div class="mb-3">
              <label for="">Price</label>
              <input type="number" name="price" class="form-control">
            </div>
            <div class="mb-3">
              <label for="">Type</label>
              <select name="type" id="" class="form-select">
                  <option value="null">Select type</option>
                  <option value="House">House</option>
                  <option value="Apartment">Apartment</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="">Area</label>
              <input type="number" name="area" class="form-control">
            </div>
            <div class="mb-3">
              <label for="">Numbers of bedroom</label>
              <select name="bedroom" id="" class="form-select">
                  <option value="null">Select</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="">Numbers of bathroom</label>
              <select name="bathroom" id="" class="form-select">
                  <option value="null">Select</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="">Numbers of garage</label>
              <select name="garage" id="" class="form-select">
                  <option value="null">Select</option>
                  <option value="1">1</option>
                  <option value="2">2</option>
                  <option value="3">3</option>
                  <option value="4">4</option>
                  <option value="5">5</option>
              </select>
            </div>
            <div class="mb-3">
              <div>
                  <label for="">Amenities</label>
              </div>
              <div class="">
                  <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox1" value="Balcony" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox1">Balcony</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox2" value="Cable" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox2">Cable Tv</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Deck" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Deck</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Parking" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Parking</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Tennis" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Tennis Courts</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Bedroom" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Bedroom</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Kitchen" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Kitchen</label>
                    </div>
                    <div class="form-check form-check-inline">
                      <input class="form-check-input" type="checkbox" id="inlineCheckbox3" value="Living Room" name="amenities">
                      <label class="form-check-label" for="inlineCheckbox3">Living Room</label>
                    </div>
                </div>
              </div>
            <div>
              <button type="submit" class="btn btn-primary">Submit</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
      

<%- include('../footer') %>



